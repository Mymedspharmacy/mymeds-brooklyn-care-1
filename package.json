  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "npm run build && cd backend && npm run build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:automated": "node automated-testing.cjs",
    "test:e2e": "node end-to-end-test.cjs",
    "test:database": "node database-test.cjs",
    "test:direct-db": "node direct-db-test.cjs",
    "test:complete-form": "node complete-form-test.cjs",
    "test:api": "node simple-api-test.cjs",
    "fix-collation": "node fix-collation.js",
    "db-maintenance": "node fix-collation.js",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "validate:prod": "node deployment/scripts/validate-production.js",
    "deploy:prod": "node deploy-production.cjs",
    "deploy:docker": "chmod +x deployment/scripts/deploy-docker.sh && ./deployment/scripts/deploy-docker.sh",
    "deploy:quick": "chmod +x deployment/scripts/quick-deploy.sh && ./deployment/scripts/quick-deploy.sh",
    "update:vps": "powershell -ExecutionPolicy Bypass -File deployment/scripts/update-vps.ps1",
    "update:vps:bash": "powershell -ExecutionPolicy Bypass -File deployment/scripts/update-vps.ps1",
    "update:vps:batch": "deployment\\scripts\\update-vps.bat",
    "deploy:vps:direct": "powershell -ExecutionPolicy Bypass -File deployment/scripts/deploy-vps-direct.ps1",
    "validate:deps": "node deployment/scripts/validate-dependencies.js",
    "setup:deps": "node deployment/scripts/validate-dependencies.js && npm run build:prod && cd backend && npm run build",
    "deploy:pm2": "chmod +x deployment/scripts/deploy-production.sh && ./deployment/scripts/deploy-production.sh pm2",
    "start:prod": "cd backend && npm run start:prod",
    "db:migrate:prod": "cd backend && npx prisma migrate deploy",
    "db:seed:prod": "cd backend && npm run seed:prod"
  },
